    "vaul": "^1.1.2",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.4",
    "@playwright/test": "^1.42.1",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.163.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
